@model ControleChamadosRedeSuporte.Models.Funcionario
@{
    ViewData["Title"] = "Sobre";
}

<h2>@ViewData["Title"]</h2>
<hr />

<div>
    <dl class="dl-horizontal">
        <dt>Grad.:</dt>
        <dd>@Html.DisplayFor(model => model.Graduacao.Grad)</dd>

        <dt>@Html.DisplayNameFor(model => model.Rg)</dt>
